{
  "printWidth": 80,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "objectProperties": "preserve",
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": true,
  "vueIndentStyle": true,
  "cssIndent": 2,
  "templateCurlySpacing": "always",
  "proseWrap": "preserve",
  "requirePragma": true,
  "insertPragma": true
}
